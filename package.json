{
  "name": "nestjs-architecture",
  "version": "1.7.2",
  "description": "Nestjs Architecture",
  "author": "Grisha Hovhanyan",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "generate:module": "hygen new module",
    "generate:queue": "hygen new queue",
    "typeorm": "npx typeorm-ts-node-commonjs -d ./libs/database/src/postgres/data-source.ts",
    "migrations:create": "npx typeorm-ts-node-commonjs migration:create ./libs/database/src/postgres/migrations/$npm_config_name",
    "migrations:generate": "npm run typeorm -- migration:generate ./libs/database/src/postgres/migrations/$npm_config_name",
    "migrations:run": "npm run typeorm -- migration:run",
    "migrations:rollback": "npm run typeorm -- migration:revert"
  },
  "dependencies": {
    "@aws-sdk/client-sns": "^3.744.0",
    "@bull-board/api": "^6.7.7",
    "@bull-board/express": "^6.7.7",
    "@bull-board/nestjs": "^6.7.7",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/common": "^11.0.9",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.9",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.9",
    "@nestjs/swagger": "^11.0.3",
    "@nestjs/typeorm": "^11.0.0",
    "@ssut/nestjs-sqs": "^3.0.1",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.40.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.8.0",
    "express-basic-auth": "^1.2.1",
    "helmet": "^8.0.0",
    "hygen": "^6.2.11",
    "ioredis": "^5.5.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.3",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "typeorm-transactional": "^0.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "@nestjs/cli": "^11.0.2",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/lodash": "^4.17.15",
    "@types/node": "^22.13.2",
    "@types/passport-jwt": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.15.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  }
}
